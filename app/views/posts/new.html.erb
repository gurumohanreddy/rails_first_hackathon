<h1>Add your Project</h1>

<%= simple_form_for @post , html:{class: "well newpost" , multipart: true } do |f| %>
   <%= f.input :title %>
   <%= f.input :description %>
   <%= f.input :youtube_link %>
   <%= f.input :image , as: :file %>
   <%= f.submit 'Add Project' , class: "btn btn-primary btn-lg " %>
<% end %>
